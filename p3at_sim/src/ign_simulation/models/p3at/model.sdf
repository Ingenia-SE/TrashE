<?xml version="1.0" ?>
<sdf version="1.5">
  <model name="p3at">
    <pose>0 0 0.180 0 0 0</pose>
    <static>false</static>
    
    <!--...................... LINK CHASSIS.......................-->
    <link name="chassis_link">
      <inertial>
        <mass>14.0</mass>
        <inertia>
          <ixx>0.147116667</ixx>
          <ixy>0.0</ixy>
          <ixz>0.0</ixz>
          <iyy>0.334951167</iyy>
          <iyz>0.0</iyz>
          <izz>0.3978345</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <box>
            <size>0.501 0.300 0.19</size>
          </box>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://p3at/meshes/chassis.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    
    <!--...................... LINK RIGHT FRONT WHEEL .......................-->
    <link name="right_front_wheel_link">
      <pose>0.125 -0.201 -0.06 1.5707 0 0</pose>
      <inertial>
        <mass>1.5</mass>
        <inertia>
          <ixx>0.005240625</ixx>
          <iyy>0.005240625</iyy>
          <izz>0.009075</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.110</radius>
            <length>0.075</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1</mu>
              <mu2>1</mu2>
              <slip1>0.5</slip1>
              <slip2>0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://p3at/meshes/wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    
    <!--...................... LINK LEFT FRONT WHEEL .......................-->
    <link name="left_front_wheel_link">
      <pose>0.125 0.201 -0.06 1.5707 0 0</pose>
      <inertial>
        <mass>1.5</mass>
        <inertia>
          <ixx>0.005240625</ixx>
          <iyy>0.005240625</iyy>
          <izz>0.009075</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.110</radius>
            <length>0.075</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1</mu>
              <mu2>1</mu2>
              <slip1>0.5</slip1>
              <slip2>0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://p3at/meshes/wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    
    <!--...................... LINK RIGHT REAR WHEEL .......................-->
    <link name="right_rear_wheel_link">
      <pose>-0.125 -0.201 -0.06 1.5707 0 0</pose>
      <inertial>
        <mass>1.5</mass>
        <inertia>
          <ixx>0.005240625</ixx>
          <iyy>0.005240625</iyy>
          <izz>0.009075</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.110</radius>
            <length>0.075</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1</mu>
              <mu2>1</mu2>
              <slip1>0.5</slip1>
              <slip2>0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://p3at/meshes/wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    
    <!--...................... LINK LEFT REAR WHEEL .......................-->
    <link name="left_rear_wheel_link">
      <pose>-0.125 0.201 -0.06 1.5707 0 0</pose>
      <inertial>
        <mass>1.5</mass>
        <inertia>
          <ixx>0.005240625</ixx>
          <iyy>0.005240625</iyy>
          <izz>0.009075</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <cylinder>
            <radius>0.110</radius>
            <length>0.075</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>1</mu>
              <mu2>1</mu2>
              <slip1>0.5</slip1>
              <slip2>0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://p3at/meshes/wheel.dae</uri>
          </mesh>
        </geometry>
      </visual>
    </link>
    
    <!--...................... LINK LIDAR .......................-->
    <link name="lidar_link">
      <pose>0 0 0.1 0 0 0</pose> <!-- SE PODRÍA HACER UN POSE RELATIVE_TO: CHASSIS -->
      <inertial>
        <pose>0 0 0.17121186 0 0 0</pose>
        <mass>0.83</mass>
        <inertia>
          <ixx>0.002853261803275034</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.002853261803275034</iyy>
          <iyz>0</iyz>
          <izz>0.0029004988906384237</izz>
        </inertia>
      </inertial>
      <collision name="collision">
        <pose>0 0 0.17121186 0 0 0</pose>
        <geometry>
          <cylinder>
            <radius>0.08360116</radius>
            <length>0.14242372</length>
          </cylinder>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://p3at/meshes/lidar_3d_v2.dae</uri>
          </mesh>
        </geometry>
      </visual>
    	<sensor name='lidar_sensor' type='gpu_lidar'>"
    		<topic>lidar_topic</topic>
    		<update_rate>10</update_rate>
    		<ray>
        		<scan>
            		<horizontal>
                		<samples>640</samples>
                		<resolution>1</resolution>
                		<min_angle>-1.396263</min_angle>
                		<max_angle>1.396263</max_angle>
            		</horizontal>
            		<vertical>
                		<samples>1</samples>
                		<resolution>0.01</resolution>
                		<min_angle>0</min_angle>
                		<max_angle>0</max_angle>
            		</vertical>
        		</scan>
        		<range>
            		<min>0.08</min>
            		<max>10.0</max>
            		<resolution>0.01</resolution>
        		</range>
    		</ray>
    		<always_on>1</always_on>
    		<visualize>true</visualize>
    	</sensor>
    </link>
    
    <!--...................... LINK KINECT .......................-->
    <link name="kinect_link">
      <pose>-0.15 0 0.15 0 0 0</pose> <!--SE PODRÍA HACER UN POSE RELATIVE_TO: CHASSIS -->
      <inertial>
        <mass>0.1</mass>
      </inertial>
      <collision name="collision">
        <geometry>
          <box>
            <size>0.073000 0.276000 0.072000</size>
          </box>
        </geometry>
      </collision>
      <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://p3at/meshes/kinect.dae</uri>
          </mesh>
        </geometry>
      </visual>
      <sensor name="kinect_sensor" type="depth">
        <update_rate>20</update_rate>
        <camera>
          <horizontal_fov>1.047198</horizontal_fov>
          <image>
            <width>640</width>
            <height>480</height>
            <format>R8G8B8</format>
          </image>
          <clip>
            <near>0.05</near>
            <far>3</far>
          </clip>
        </camera>
      </sensor>
    </link>
    
    <!--...................... JOINT RIGHT FRONT WHEEL TO CHASSIS .......................-->
    <joint type="revolute" name="right_front_wheel_chassis_joint">
      <pose>0 0 0 0 0 0</pose>
      <child>right_front_wheel_link</child>
      <parent>chassis_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
    </joint>
    
    <!--...................... JOINT RIGHT REAR WHEEL TO CHASSIS .......................-->
    <joint type="revolute" name="right_rear_wheel_chassis_joint">
      <pose>0 0 0 0 0 0</pose>
      <child>right_rear_wheel_link</child>
      <parent>chassis_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
    </joint>
    
    <!--...................... JOINT LEFT FRONT WHEEL TO CHASSIS .......................-->
    <joint type="revolute" name="left_front_wheel_chassis_joint">
      <pose>0 0 0 0 0 0</pose>
      <child>left_front_wheel_link</child>
      <parent>chassis_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
    </joint>
    
    <!--...................... JOINT LEFT REAR WHEEL TO CHASSIS .......................-->
    <joint type="revolute" name="left_rear_wheel_chassis_joint">
      <pose>0 0 0 0 0 0</pose>
      <child>left_rear_wheel_link</child>
      <parent>chassis_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
    </joint>
    
    <!--...................... JOINT LIDAR TO CHASSIS .......................-->
    <joint type="fixed" name="lidar_chassis_joint">
      <pose>0 0 0 0 0 0</pose>
      <child>lidar_link</child>
      <parent>chassis_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
    </joint>
    
    <!--...................... JOINT KINECT TO CHASSIS .......................-->
    <joint type="fixed" name="kinect_chassis_joint">
      <pose>0 0 0 0 0 0</pose>
      <child>kinect_link</child>
      <parent>chassis_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
    </joint>
    
    
    <!-- FALTA INSTALARLO
    <plugin name="SkidSteerDrivePlugin" filename="libSkidSteerDrivePlugin.so">
      <right_front>right_front</right_front>
      <right_rear>right_rear</right_rear>
      <left_front>left_front</left_front>
      <left_rear>left_rear</left_rear>
      <MaxForce>5.0</MaxForce>
    </plugin>
    -->
    
    
    <!--...................... PLUGIN TO USE LIDAR .......................-->
    <plugin filename="libignition-gazebo-sensors-system.so"
      name="ignition::gazebo::systems::Sensors">
      <render_engine>ogre2</render_engine>
    </plugin>
    
    <!--...................... PLUGIN TO USE DIFFERENTIAL DRIVE .......................-->
    <!--
    ign topic -t "/cmd_vel" -m ignition.msgs.Twist -p "linear: {x:0.5}, angular: {z:0.05}"
    -->
    <plugin filename="libignition-gazebo-diff-drive-system.so" name="ignition::gazebo::systems::DiffDrive">
      <left_joint>left_front_wheel_chassis_joint</left_joint>
      <left_joint>left_rear_wheel_chassis_joint</left_joint>
      <right_joint>right_front_wheel_chassis_joint</right_joint>
      <right_joint>right_rear_wheel_chassis_joint</right_joint>
      <wheel_separation>0.402</wheel_separation>
      <wheel_radius>0.110</wheel_radius>
      <odom_publish_frequency>1000</odom_publish_frequency>
      <topic>/cmd_vel</topic>
    </plugin>
    
    <!--...................... PLUGIN TO PUBLISH ROBOT STATE INFORMATION .......................-->
    <plugin filename="libignition-gazebo-pose-publisher-system.so" name="ignition::gazebo::systems::PosePublisher">
      <publish_link_pose>true</publish_link_pose>
      <publish_sensor_pose>true</publish_sensor_pose>
      <publish_collision_pose>false</publish_collision_pose>
      <publish_visual_pose>false</publish_visual_pose>
      <publish_nested_model_pose>true</publish_nested_model_pose>
      <use_pose_vector_msg>true</use_pose_vector_msg>
      <static_publisher>true</static_publisher>
      <static_update_frequency>1</static_update_frequency>
    </plugin>
    
    
